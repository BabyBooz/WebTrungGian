                                <div readonly="" class="left-align-text form-control">
                                    <span><c:choose>
                                            <c:when test="${productDetail3.status_fee == 0}">
                                                ${productDetail3.price}
                                            </c:when>
                                            <c:when test="${productDetail3.status_fee == 1}">
                                                ${productDetail3.price - (productDetail3.price*(1/10))} 
                                            </c:when>
                                            <c:otherwise>Unknown</c:otherwise>
                                        </c:choose>
